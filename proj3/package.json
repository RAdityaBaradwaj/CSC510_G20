{
  "name": "proj2",
  "version": "1.0.0",
  "description": "E-commerce application with nearby orders board",
  "main": "index.js",
  "scripts": {
    "test": "vitest --config vitest.config.js",
    "test:ui": "vitest --ui",
    "test:backend": "vitest --run --config vitest.config.js backend/tests",
    "server": "ts-node backend/src/server.ts",
    "db:migrate": "node backend/scripts/runMigrations.js",
    "db:seed": "node backend/scripts/seedRestaurants.js",
    "db:reset": "npm run db:migrate && npm run db:seed",
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview"
  },
  "keywords": [
    "ecommerce",
    "nearby-orders",
    "location-based"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.3.0",
    "express": "^4.19.2",
    "leaflet": "^1.9.3",
    "pg": "^8.11.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-leaflet": "^4.2.1",
    "react-router-dom": "^6.8.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.6",
    "@vitejs/plugin-react": "^3.1.0",
    "@vitest/coverage-c8": "^0.28.5",
    "jsdom": "^21.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "vite": "^4.1.0",
    "vitest": "^0.28.0"
  }
}
